<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <!-- 教务管理模块逆向工程配置 -->
    <context id="education" targetRuntime="MyBatis3">
        <!-- 设置分隔符 -->
        <property name="beginningDelimiter" value="`"/>
        <property name="endingDelimiter" value="`"/>

        <!-- 取消所有注释 -->
        <commentGenerator>
            <property name="suppressAllComments" value="true"/>
            <property name="suppressDate" value="true"/>
        </commentGenerator>

        <!-- 配置数据库连接参数 -->
        <jdbcConnection driverClass="oracle.jdbc.OracleDriver"
                        connectionURL="jdbc:oracle:thin:@localhost:1521:orcl"
                        userId="ruicai"
                        password="ruicai"/>

        <!-- 配置实体类的生成 -->
        <javaModelGenerator targetPackage="com.ruicai.system.po.education" targetProject="SystemManagement/src/main/java">
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <!-- 配置教务管理模块mapper.xml文件的生成 -->
        <sqlMapGenerator targetPackage="mybatis.mapper.education" targetProject="SystemManagement/src/main/resources"/>

        <!-- 配置教务管理模块Mapper接口的生成 -->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.ruicai.system.dao.education" targetProject="SystemManagement/src/main/java"/>

        <!-- 配置教务管理模块所有表的生成 -->
        <table schema="ruicai" tableName="EDU%"/>
    </context>

